<% include('partials/header.ejs') %>
<div class="container">
    <% games.forEach(v=> { %>
        <div class="cards">
            <img src="<%=v.background_image %>" height="150" />
            <p><%=v.name %></p>
            <a href="/games/<%=v.id %>/view" class="btn btn-info">View</a>
            <form action="/games/<%=v.id %>?_method=DELETE" method="POST">
                <input class="btn btn-danger" type="submit" value="Delete">
            </form>
        </div>
    <% }) %>
</div>